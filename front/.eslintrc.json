{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "for-direction": "error",
    "no-compare-neg-zero": "error",
    "no-cond-assign": "error",
    "no-constant-condition": "error",
    "no-empty": "error",
    "no-extra-semi": "error",
    "no-func-assign": "error",
    "no-unexpected-multiline": "warn",
    "no-unreachable": "error",
    "no-unsafe-finally": "error",
    "use-isnan": "error",
    "valid-typeof": "warn",
    "curly": "warn",
    "no-case-declarations": "warn",
    "no-empty-pattern": "warn",
    "no-fallthrough": "warn",
    "no-redeclare": "warn",
    "no-self-compare": "warn",
    "no-useless-escape": "warn",
    "no-delete-var": "warn",
    "no-unused-vars": "warn",
    "max-len": ["warn", {
      "code": 100,
      "comments": 100
    }],
    "max-nested-callbacks": ["error", 3],
    "no-mixed-spaces-and-tabs": "error",
    "no-tabs": "error",
    "semi-style": ["error", "last"],
    "space-infix-ops": "warn",
    "arrow-parens": "error",
    "no-var": "error",
    "prefer-template": "warn",
    "prefer-const": "warn",
    "no-inline-comments": "warn",
    "comma-style": ["warn", "last"],
    "comma-spacing": ["warn", {
      "before": false,
      "after": true
    }],
    "camelcase": ["warn", {
      "properties": "always"
    }]
  }
}
